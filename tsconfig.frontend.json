{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "composite": true,
        "sourceMap": true,
        "jsx": "react",
        "allowJs": true,
        "paths": {
            "components/*": [
                "./frontend/components/*"
            ],
            "services/*": [
                "./frontend/services/*"
            ],
        }
    },
    "include": [
        "./frontend/**/*"
    ]
}
